{
  "name": "argos",
  "version": "0.1.0",
  "description": "Argos is a visual diffing tool made with love",
  "main": "index.js",
  "private": true,
  "scripts": {
    "console": "babel-node src/scripts/repl.js",
    "test:unit": "jest --runInBand",
    "test:watch": "yarn test:unit --watch",
    "web:dev": "heroku local -f Procfile.dev -p 4001",
    "web:build": "rm -rf lib/ && cp -R src/ lib/ && babel -d lib src",
    "review:dev": "webpack-dev-server",
    "review:build": "rm -rf server/static/review && NODE_ENV=production webpack",
    "review:icons": "babel-node src/scripts/generateIcons.js",
    "review:compress": "open -a ImageOptim src server/public",
    "argos": "argos upload screenshots/samples --token $ARGOS_TOKEN || true",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "api:db:console": "docker-compose run postgres psql -h postgres -U postgres",
    "api:db:create": "babel-node src/scripts/db/create.js",
    "api:db:migrate": "knex migrate:latest",
    "api:db:drop": "babel-node src/scripts/db/drop.js",
    "api:db:dump": "babel-node src/scripts/db/dump.js",
    "api:db:load": "babel-node src/scripts/db/load.js",
    "api:db:reset": "yarn api:db:drop && yarn api:db:create && yarn api:db:load",
    "api:db:truncate": "babel-node src/scripts/db/truncate.js",
    "heroku-postbuild": "yarn web:build && yarn review:build",
    "prettier": "find . -name \"*.js\" | grep -v -f .eslintignore | xargs prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argos-ci/argos.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/argos-ci/argos/issues"
  },
  "engines": {
    "node": "10"
  },
  "homepage": "https://github.com/argos-ci/argos#readme",
  "dependencies": {
